class Solution {
public:
    bool isAnagram(string s, string t) 
    {
        
        sort(s.begin(),s.end());
        sort(t.begin(),t.end());
        
        int k= s.compare(t);
        
        if(k==0)
        {
            return true;
        }

        else
        {
            return false;
        }
    
        
    }
};
